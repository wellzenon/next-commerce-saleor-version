{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch firefox",
      "type": "firefox",
      "request": "attach",
      "pathMappings": [
        {
          "url": "webpack://_n_e",
          "path": "${workspaceFolder}"
        },
        {
          "url": "webpack://_n_e/framework/commerce/index.tsx",
          "path": "${workspaceFolder}/pages/auth/[provider]/index.tsx"
        },
        {
          "url": "webpack://_n_e",
          "path": "${workspaceFolder}"
        }
      ]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Next: Node",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next"
    }
  ]
}
